{
  "name": "electron-updater-example",
  "version": "1.1.2",
  "main": "main.js",
  "description": "electron-updater example project",
  "scripts": {
    "dist-win": "electron-builder --publish never --win",
    "dist-mac": "electron-builder --publish never --mac",
    "publish": "electron-builder -p always"
  },
  "devDependencies": {
    "electron": "^26.1.0",
    "electron-builder": "^22.11.7",
    "electron-notarize": "^1.1.1"
  },
  "dependencies": {
    "aws-sdk": "^2.1458.0",
    "aws4": "^1.12.0",
    "config": "^3.3.9",
    "electron-log": "^4.2.4",
    "electron-updater": "^6.1.4",
    "fs": "^0.0.1-security"
  },
  "build": {
    "publish": {
      "provider": "s3",
      "bucket": "bhavanish1",
      "path": "clockin/",
      "region": "ap-south-1"
      
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "target": [
        "zip",
        "pkg",
        "dmg"
      ]
    },
    "linux": {
      "target": []
    },
    "snap": false,
    "nsis": {
      "perMachine": true,
      "oneClick": false,
      "runAfterFinish": false
    }
  }
}
